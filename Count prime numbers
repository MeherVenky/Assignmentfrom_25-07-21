import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
int s=0,r=0;
int count,c,a,sum=0;
        int n = sc.nextInt();
for(int k=0;k<n;k++){

 s = sc.nextInt();    

 r= sc.nextInt();

int m=0;
for(int i = s ; i<=r ; i++)
{
count = 0;
for(int j = 1; j <= i ; j++)
{
if(i % j == 0)
count = count+1;

}
if(count == 2)
m++;

}

System.out.println(m);
}
    }
}
